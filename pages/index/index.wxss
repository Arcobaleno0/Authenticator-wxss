.body{
  margin: 0;
}
.body:before {
    background-color: rgb(242,242,242);
    width: 100%;
    content: '';
    position: fixed;
    display: block;
    bottom: 0;
    top: 0;
    z-index: -1;
}
.ul{
  margin: 0;
  padding: 0;
}
.li{
  list-style: none;
}
.auth{
  margin-bottom: 5em;
}
.auth .h1{
  margin: 0 auto;
}
.auth .ul .li{
  margin-top: 1.5em;
  background-color: #ffffff;
}
.auth .ul .li.list{
  padding: 0.8em;
}
.auth .ul .li .token{
  color: rgb(66,133,244);
}
.auth .ul .li .info{
  padding: 0.4em 0;
}
.am-cf:after, .am-cf:before {
    content: " ";
    display: table;
}
.am-cf:after {
    clear: both;
}
.am-fl {
    float: left;
}
.am-fr {
    float: right;
}
.onlineService {
    position: fixed;
    right: 1em;
    bottom: 1em;
    width: 3.5rem;
    height: 3.5rem;
    background-color: rgb(221,44,1);
    border-radius: 100%;
    z-index: 9999;
}
.add {
    color: #ffffff;
    width: 3.5rem;
    height: 3.5rem;
    line-height:3.5rem; 
    font-size: 2rem;
    text-align: center;
    white-space: nowrap;
    vertical-align: middle;
}