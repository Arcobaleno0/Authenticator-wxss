@import "../../styles/md.wxss"
.body{
  margin: 0;
}
.body:before {
    background-color: rgb(250,250,250);
    width: 100%;
    content: '';
    position: fixed;
    display: block;
    bottom: 0;
    top: 0;
    z-index: -1;
}
.hide{
  display: none;
}
.am-cf:after, .am-cf:before {
    content: " ";
    display: table;
}
.am-cf:after {
    clear: both;
}
.am-fl {
    float: left;
}
.am-fr {
    float: right;
}
.mdl-textfield{
    width:100%;
}
.mdl-textfield__input{
    color:#000000;
}